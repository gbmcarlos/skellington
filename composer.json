{
    "name": "gbmcarlos/skellington-laravel",
    "version": "0.1",
    "require": {
        "php": "^7.2",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.6.*"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/app/",
            "Skeleton\\": "skeleton/"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "Skeleton\\SkeletonTemplateReplacer::run",
            "Skeleton\\SkeletonFolderRemover::run"
        ]
    }
}